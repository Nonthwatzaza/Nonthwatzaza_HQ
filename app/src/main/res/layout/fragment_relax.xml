<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Relative_relax"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nzoshq.nonthwatzaza.nonthwatzaza_hq.relaxFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="สบายๆ ชิวๆกันเถอะ  ~(˘▾˘~)"
        android:textSize="25dip"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:onClick="openyoutube"
        android:adjustViewBounds="true"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="23dp"
        app:srcCompat="@drawable/google_youtube_1"
        android:layout_below="@+id/textView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:onClick="mxplayer"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/mx_player"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:onClick="poweramp"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/poweramp"
        android:layout_alignTop="@+id/imageButton5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ที่มันดูหยาบๆเพราะมันเป็นbetaอยู่นะจ๊ะ =="
        android:textSize="20dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
