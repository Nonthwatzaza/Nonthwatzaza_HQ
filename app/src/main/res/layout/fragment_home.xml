<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nzoshq.nonthwatzaza.nonthwatzaza_hq.homeFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ยินดีต้อนรับจ้าาาา"
        android:textSize="20dip"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="120dp"
        android:layout_height="120dp"
        app:srcCompat="@drawable/non2"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="Nonthwat zaza"
        android:textSize="25dip"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView2"
        android:text="ตามสบายเลยน้าาาา ^-^"
        android:textSize="10dip"
        android:textStyle="" />

    <TableRow
    android:id="@+id/hr3"
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:background="#444"
        android:layout_alignTop="@+id/imageView8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"></TableRow>

    <TableRow
        android:id="@+id/hr1"
        android:layout_width="1dp"
        android:layout_height="250dp"
        android:background="#444"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"></TableRow>

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Newbie androidDev"
        android:textSize="12dp"
        android:layout_below="@+id/textView15"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Knw#46 #Dek61"
        android:textSize="12dp"
        android:layout_below="@+id/textView16"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/acc_home"
        android:adjustViewBounds="true"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/hr1"
        android:layout_toEndOf="@+id/hr1" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/camera_home"
        android:adjustViewBounds="true"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/imageView8"
        android:layout_toStartOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/home_relax"
        android:adjustViewBounds="true"
        android:layout_alignBottom="@+id/hr3"
        android:layout_toRightOf="@+id/hr1"
        android:layout_toEndOf="@+id/hr1" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="truephonecall"
        app:srcCompat="@drawable/phone_home"
        android:adjustViewBounds="true"
        android:layout_alignTop="@+id/imageView11"
        android:layout_toLeftOf="@+id/imageView8"
        android:layout_toStartOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="43dp"
        android:text="โทรศัพท์"
        android:layout_above="@+id/imageView10"
        android:layout_toLeftOf="@+id/textView16"
        android:layout_toStartOf="@+id/textView16"
        android:textSize="15dp"
        android:textColor="#ffffff"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Relax"
        android:textSize="15dp"
        android:textColor="#ffffff"
        android:layout_alignBaseline="@+id/textView18"
        android:layout_alignBottom="@+id/textView18"
        android:layout_toRightOf="@+id/textView15"
        android:layout_toEndOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView18"
        android:layout_marginBottom="43dp"
        android:text="กล้องถ่ายรูป"
        android:textSize="15dp"
        android:textColor="#ffffff"/>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:text="เครื่องมือ"
        android:textSize="15dp"
        android:textColor="#ffffff"/>

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="์NonthwatzazaHQ beta v1.0
"
        android:layout_alignTop="@+id/hr1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView12"
        android:layout_toLeftOf="@+id/imageView8"
        android:background="#00000000"
        android:onClick="camera"
        app:srcCompat="@android:color/transparent" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageView11"
        android:background="#00000000"
        android:onClick="truephonecall"
        app:srcCompat="@android:color/transparent" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/hr3"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/imageView11"
        android:layout_toEndOf="@+id/hr1"
        android:layout_toRightOf="@+id/hr1"
        android:background="#00000000"
        app:srcCompat="@android:color/transparent" />


</RelativeLayout>
